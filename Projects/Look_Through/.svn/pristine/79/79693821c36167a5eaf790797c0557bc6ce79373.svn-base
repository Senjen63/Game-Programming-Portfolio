#include "givepower.h"

GivePower::GivePower(SnakeManager* snake, PowerUpManager* food) : GameEvent(POWERUP)
{
	for each (Unit * unis in food->getUnits())
	{
		if (snake->getFirstPos() == unis->getPosition())
		{
			mEating = true;
		}
	}
}

GivePower::~GivePower()
{

