#include "LoadGameButton.h"

LoadGameButton::LoadGameButton(std::pair<float, float>location, std::pair<float, float>scale, string text, string animationDataFilePath) : Button(location, scale, text, animationDataFilePath)
{

}

LoadGameButton::~LoadGameButton()
{

}

void LoadGameButton::onClicked()
{
	Game::getGame()->getMenuManager()->unloadMenu();
	Game::getGame()->loadGame();
}