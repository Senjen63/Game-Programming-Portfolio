#include "food.h"

Food::Food(SnakeManager* snake, FoodManager* food) : GameEvent(FOOD)
{
	for each (Unit* unis in food->getUnits())
	{
		if (snake->getFirstPos() == unis->getPosition())
		{
			mEating = true;
		}
	}
}

