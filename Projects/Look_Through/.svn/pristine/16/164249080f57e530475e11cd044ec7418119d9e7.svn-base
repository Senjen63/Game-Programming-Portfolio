#include "..\Pieces\Queen.h"
#include "../Game.h"

Queen::Queen()
{

}

Queen::Queen(GraphicsBuffer* pBuffer, Vector2D location)
{
	setPosition(location);
	setSprite(new Sprite(pBuffer, Vector2D(), Game::getInstance()->getSpriteWidth(), Game::getInstance()->getSpriteHeight()));

	createMovePts();
}

Queen::~Queen()
{
	cleanUp();
}

void Queen::cleanUp()
{
	for (unsigned int i = 0; i < mpMovePts.size(); i++)
	{
		Unit* pUnit = mpMovePts[i];
		delete pUnit;
	}

	for (unsigned int i = 0; i < mpAttackPts.size(); i++)
	{
		Unit* pUnit = mpAttackPts[i];
		delete pUnit;
	}

	mpMovePts.clear();
	mpAttackPts.clear();
}

void Queen::createMovePts()
{
	Unit* moveUnit;
	int i;

	//Diagonal Movement
	for (i = 1; i <= 7; i++)
	{
		moveUnit = new Unit(Game::getInstance()->getAnimation(Game::getInstance()->getMoveString()), Vector2D(80 * i, 80 * i) + getPosition());
		mpMovePts.push_back(moveUnit);
		moveUnit = new Unit(Game::getInstance()->getAnimation(Game::getInstance()->getAttackString()), Vector2D(80 * i, 80 * i) + getPosition());
		mpAttackPts.push_back(moveUnit);
	}
	for (i = 1; i <= 7; i++)
	{
		moveUnit = new Unit(Game::getInstance()->getAnimation(Game::getInstance()->getMoveString()), Vector2D(-80 * i, 80 * i) + getPosition());
		mpMovePts.push_back(moveUnit);
		moveUnit = new Unit(Game::getInstance()->getAnimation(Game::getInstance()->getAttackString()), Vector2D(-80 * i, 80 * i) + getPosition());
		mpAttackPts.push_back(moveUnit);
	}
	for (i = 1; i <= 7; i++)
	{
		moveUnit = new Unit(Game::getInstance()->getAnimation(Game::getInstance()->getMoveString()), Vector2D(80 * i, -80 * i) + getPosition());
		mpMovePts.push_back(moveUnit);
		moveUnit = new Unit(Game::getInstance()->getAnimation(Game::getInstance()->getAttackString()), Vector2D(80 * i, -80 * i) + getPosition());
		mpAttackPts.push_back(moveUnit);
	}
	for (i = 1; i <= 7; i++)
	{
		moveUnit = new Unit(Game::getInstance()->getAnimation(Game::getInstance()->getMoveString()), Vector2D(-80 * i, -80 * i) + getPosition());
		mpMovePts.push_back(moveUnit);
		moveUnit = new Unit(Game::getInstance()->getAnimation(Game::getInstance()->getAttackString()), Vector2D(-80 * i, -80 * i) + getPosition());
		mpAttackPts.push_back(moveUnit);
	}

	//Straight line
	for (i = 1; i <= 7; i++)
	{
		moveUnit = new Unit(Game::getInstance()->getAnimation(Game::getInstance()->getMoveString()), Vector2D(0, 80 * i) + getPosition());
		mpMovePts.push_back(moveUnit);
		moveUnit = new Unit(Game::getInstance()->getAnimation(Game::getInstance()->getAttackString()), Vector2D(0, 80 * i) + getPosition());
		mpAttackPts.push_back(moveUnit);
	}

	for (i = 1; i <= 7; i++)
	{
		moveUnit = new Unit(Game::getInstance()->getAnimation(Game::getInstance()->getMoveString()), Vector2D(0, -80 * i) + getPosition());
		mpMovePts.push_back(moveUnit);
		moveUnit = new Unit(Game::getInstance()->getAnimation(Game::getInstance()->getAttackString()), Vector2D(0, -80 * i) + getPosition());
		mpAttackPts.push_back(moveUnit);
	}

	for (i = 1; i <= 7; i++)
	{
		moveUnit = new Unit(Game::getInstance()->getAnimation(Game::getInstance()->getMoveString()), Vector2D(80 * i, 0) + getPosition());
		mpMovePts.push_back(moveUnit);
		moveUnit = new Unit(Game::getInstance()->getAnimation(Game::getInstance()->getAttackString()), Vector2D(80 * i, 0) + getPosition());
		mpAttackPts.push_back(moveUnit);
	}

	for (i = 1; i <= 7; i++)
	{
		moveUnit = new Unit(Game::getInstance()->getAnimation(Game::getInstance()->getMoveString()), Vector2D(-80 * i, 0) + getPosition());
		mpMovePts.push_back(moveUnit);
		moveUnit = new Unit(Game::getInstance()->getAnimation(Game::getInstance()->getAttackString()), Vector2D(-80 * i, 0) + getPosition());
		mpAttackPts.push_back(moveUnit);
	}
}

