#include "Color.h"

Color::Color( unsigned int r, unsigned int g, unsigned int b )
{
    mR = r;
    mG = g;
    mB = b;
    mA = 255;
}
Color::Color( unsigned int r, unsigned int g, unsigned int b, float a )
{
    mR = r;
    mG = g;
    mB = b;
    mA = a;
}
Color::~Color()
{
    
}
int Color::getR()
{
    return mR;
}
int Color::getG()
{
    return mG;
}
int Color::getB()
{
    return mB;
}
float Color::getA()
{
    return mA;
}