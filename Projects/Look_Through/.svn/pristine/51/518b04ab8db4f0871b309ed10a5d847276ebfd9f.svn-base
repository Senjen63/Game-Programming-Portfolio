#include "PowerUps.h"
#include "Unit.h"

PowerUps::PowerUps()
{

}

PowerUps::PowerUps(string name)
{
	mpPower = new Unit(Vector2D(rand() % (DISPLAY_WIDTH - 120), rand() % (DISPLAY_HEIGHT - 120)), name);
	name = mName;
}

PowerUps::~PowerUps()
{
	delete mpPower;
	mpPower = nullptr;
}

string PowerUps::getName()
{
	return mName;
}