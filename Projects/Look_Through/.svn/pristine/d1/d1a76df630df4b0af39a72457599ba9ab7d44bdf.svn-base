#pragma once

#include <Event.h>
#include "InputEvent.h"


enum class GameEventType
{
	SCORE_CHANGE_EVENT = (int)InputEventType::NUM_INPUT_EVENT_TYPES,
	SECOND_PASSED_EVENT,
	OBJECT_COLLISION_EVENT,
	PLAYER_ACTION_EVENT,
	GAME_OVER_EVENT,
	LEVEL_COMPLETE_EVENT,
	NUM_GAME_EVENT_TYPES
};

class GameEvent : public Event
{
public:

	GameEvent(GameEventType type) : Event((EventType)type) {};
	virtual ~GameEvent() {};

private:
};

