#include "power.h"

Power::Power(SnakeManager* snake, PowerUpManager* food) : GameEvent(POWERUP)
{
	for each (Unit * unis in food->getUnits())
	{
		if (snake->getFirstPos() == unis->getPosition())
		{
			mEating = true;
		}
	}
}

